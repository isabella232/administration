{% block sw_form_field_renderer %}
<component
    :is="componentName"
    v-bind="bind"
    ref="component"
    v-model="currentValue"
    class="sw-form-field-renderer"
    @change="emitChange"
>
    {% block sw_form_field_renderer_scope_slots %}
    <template
        v-for="(slot, slotName) in $scopedSlots"
        v-slot:[slotName]="slotData"
    >
        <slot
            v-if="feature.isActive('FEATURE_NEXT_17973')"
            :name="slotName"
            v-bind="slotData"
        >
        </slot>
    </template>
    {% endblock %}

    {% block sw_form_field_renderer_inner %}
    <slot>{% block sw_form_field_renderer_inner_slot %}{% endblock %}</slot>
    {% endblock %}
</component>
{% endblock %}
