{% block sw_settings_tax_area_rule_type_zip_code_range %}
    <sw-container columns="1fr 1fr" gap="0 32px">
        {% block sw_settings_tax_area_rule_type_zip_code_range_from %}
            <sw-text-field v-model="taxAreaRule.data.fromZipCode" :label="$tc('sw-settings-tax.taxAreaRuleCard.labelToZipCode')">
            </sw-text-field>
        {% endblock %}

        {% block sw_settings_tax_area_rule_type_zip_code_range_to %}
            <sw-text-field v-model="taxAreaRule.data.toZipCode" :label="$tc('sw-settings-tax.taxAreaRuleCard.labelFromZipCode')">
            </sw-text-field>
        {% endblock %}
    </sw-container>
{% endblock %}
