{% block sw_condition_fields %}
    {% block sw_condition_fields_operator %}
        <sw-select name="operator"
                   id="createId()"
                   itemValueKey="identifier"
                   displayName="label"
                   :store="operators"
                   :required="true"
                   v-model="condition.value.operator"
                   class="field field--select has--arrow">
        </sw-select>
    {% endblock %}
    {% block sw_condition_fields_value %}
        <sw-field class="field field--main"
                  name="streetName"
                  v-model="condition.value.streetName">
        </sw-field>
    {% endblock %}
{% endblock %}
