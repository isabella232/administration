{% block sw_config_form %}
    <sw-page class="sw-config-form-renderer">
        {% block sw_config_form_card_view %}
            <sw-card-view slot="content">
                {% block sw_config_form_cards %}
                    <sw-card v-for="card, index in config"
                             :key="index"
                             :title="card.title[locale]">
                        <sw-field v-for="field in card.fields"
                                  :key="field.name"
                                  :type="field.type"
                                  v-model="field.value"
                                  :disabled=field.disabled
                                  :label="getLabel(field)"
                                  :placeholder="getPlaceholder(field)"
                                  :copyAble=field.copyable
                                  :options="getOptions(field)"
                                  :helpText="getHelpText(field)">
                        </sw-field>
                    </sw-card>
                {% endblock %}
            </sw-card-view>
        {% endblock %}
    </sw-page>
{% endblock %}